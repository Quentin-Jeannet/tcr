{% extends 'base-admin.html.twig' %}

{% block title %}FloorFamily{% endblock %}

{% block body %}
    <h1>FloorFamily</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ floor_family.id }}</td>
            </tr>
            <tr>
                <th>Text</th>
                <td>{{ floor_family.text }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ floor_family.createdAt ? floor_family.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ floor_family.updatedAt ? floor_family.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Rank</th>
                <td>{{ floor_family.rankOrder }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ floor_family.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_floor_family_index') }}">back to list</a>

    <a href="{{ path('app_admin_floor_family_edit', {'id': floor_family.id}) }}">edit</a>

    {{ include('admin_floor_family/_delete_form.html.twig') }}
{% endblock %}
