{% extends 'base-admin.html.twig' %}

{% block title %}SocialNetwork{% endblock %}

{% block body %}
    <h1>SocialNetwork</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ social_network.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ social_network.name }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ social_network.link }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ social_network.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_social_network_index') }}">back to list</a>

    <a href="{{ path('app_admin_social_network_edit', {'id': social_network.id}) }}">edit</a>

    {{ include('admin_social_network/_delete_form.html.twig') }}
{% endblock %}
